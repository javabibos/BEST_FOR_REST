<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sasha.best_for_rest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PhilippinesActivity"
            android:label="@string/title_activity_philippines"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TailandActivity"
            android:label="@string/title_activity_tailand"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CambodiaActivity"
            android:label="@string/title_activity_cambodia"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CostaRikaActivity"
            android:label="@string/title_activity_costa_rika"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BelizActivity"
            android:label="@string/title_activity_beliz"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
